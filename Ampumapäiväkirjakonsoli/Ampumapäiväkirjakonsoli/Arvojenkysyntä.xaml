<Window x:Class="Ampumapäiväkirjakonsoli.Arvojenkysyntä"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ampumapäiväkirjakonsoli"
        mc:Ignorable="d"
        Title="Arvojenkysyntä" Height="450" Width="800">
    <Grid>
        <Grid>
            <Label x:Name="lblKysymys" HorizontalAlignment="Center" Margin="0,30,0,0" VerticalAlignment="Top" Width="800" Height="50"/>
            <TextBox x:Name="txtVastaus1" HorizontalAlignment="Center" Margin="0,100,0,0" VerticalAlignment="Top" Width="200" Height="50"/>
            <Button Content="Valmis" HorizontalAlignment="Center" Margin="0,250,0,0" VerticalAlignment="Top" Width="96" Height="48" Click="Valmis_Click"/>
            <Button Content="Seuraava" HorizontalAlignment="Center" Margin="0,346,0,0" VerticalAlignment="Top" Click="SeuraavaButton_Click"/>

        </Grid>
    </Grid>
</Window>
