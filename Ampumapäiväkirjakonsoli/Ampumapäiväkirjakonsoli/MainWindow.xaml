<Window x:Class="Ampumapäiväkirjakonsoli.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ampumapäiväkirjakonsoli"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1100">
    <Grid>
        <TextBox x:Name="txtNykyinenAika" Margin="599,53,375,776" Grid.Row="0" Background="#78866B"/>
        <TextBox HorizontalAlignment="Center" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Center" Width="688" Height="616" Background="#78866B"/>
        <Button Content="Aloita tulosten kirjaaminen" HorizontalAlignment="Left" Margin="242,53,0,0" VerticalAlignment="Top" Height="55" Width="128" Background="#78866B" Click="AloitaKirjaaminen_Click"/>
        <Button Content="Näytä tulokset" HorizontalAlignment="Left" Margin="423,53,0,0" VerticalAlignment="Top" Width="127" Height="55" Background="#78866B" Click="NäytäTulokset_Click"/>
        <TextBox HorizontalAlignment="Left" Margin="1283,23,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="143" Height="60" Background="#78866B"/>
        <Button Content="Sulje ohjelma" HorizontalAlignment="Left" Margin="763,53,0,0" VerticalAlignment="Top" Height="55" Width="131" Click="SuljeOhjelma_Click"/>
    </Grid>
</Window>
